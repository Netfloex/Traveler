{
	"trailingComma": "none",
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 4,
	"printWidth": 80,
	"arrowParens": "always",
	"importOrderSeparation": true,
	"importOrder": [
		".*scss$",
		"^[\\w/-]+$",
		"^@mui/.*",
		"^@hooks$",
		"^(@lib)|(@utils)|(@server)$",
		"^(@|\\./)components/*",
		"^\\./.*",
		"^@client/.*",
		"^@endpoints/.*",
		"^@typings/.*",
		"^@seo-default$"
	]
}
